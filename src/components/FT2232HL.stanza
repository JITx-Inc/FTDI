#use-added-syntax(jitx)
defpackage FTDI/components/FT2232HL :
  import core
  import jitx
  import jitx/commands

  import jsl/symbols
  import jsl/landpatterns

public pcb-component component () :

  manufacturer = "Future Technology Devices International"
  mpn = "FT2232HL"
  datasheet = "https://ftdichip.com/wp-content/uploads/2024/05/DS_FT2232H.pdf"
  reference-prefix = "U"

  pin-properties:
    [ pin:Ref | pads:Int ... | side:Dir ]
    [ GND     | 1 5 11 15 25 35 47 51 | Down ]
    [ AGND    | 10           | Down     ]

    [ VCORE   | 12 37 64     | Up       ]
    [ VCCIO   | 20 31 42 56  | Up       ]
    [ VPLL    | 9            | Up       ]
    [ VPHY    | 4            | Up       ]

    [ VREGIN  | 50           | Left     ]
    [ VREGOUT | 49           | Left     ]
    [ DM      | 7            | Left     ]
    [ DP      | 8            | Left     ]
    [ REF     | 6            | Left     ]
    [ RESET   | 14           | Left     ]
    [ EECS    | 63           | Left     ]
    [ EECLK   | 62           | Left     ]
    [ EEDATA  | 61           | Left     ]
    [ OSCI    | 2            | Left     ]
    [ OSCO    | 3            | Left     ]
    [ TEST    | 13           | Left     ]

    [ ADBUS0  | 16           | Right    ]
    [ ADBUS1  | 17           | Right    ]
    [ ADBUS2  | 18           | Right    ]
    [ ADBUS3  | 19           | Right    ]
    [ ADBUS4  | 21           | Right    ]
    [ ADBUS5  | 22           | Right    ]
    [ ADBUS6  | 23           | Right    ]
    [ ADBUS7  | 24           | Right    ]

    [ ACBUS0  | 26           | Right    ]
    [ ACBUS1  | 27           | Right    ]
    [ ACBUS2  | 28           | Right    ]
    [ ACBUS3  | 29           | Right    ]
    [ ACBUS4  | 30           | Right    ]
    [ ACBUS5  | 32           | Right    ]
    [ ACBUS6  | 33           | Right    ]
    [ ACBUS7  | 34           | Right    ]

    [ BDBUS0  | 38           | Right    ]
    [ BDBUS1  | 39           | Right    ]
    [ BDBUS2  | 40           | Right    ]
    [ BDBUS3  | 41           | Right    ]
    [ BDBUS4  | 43           | Right    ]
    [ BDBUS5  | 44           | Right    ]
    [ BDBUS6  | 45           | Right    ]
    [ BDBUS7  | 46           | Right    ]

    [ BCBUS0  | 48           | Right    ]
    [ BCBUS1  | 52           | Right    ]
    [ BCBUS2  | 53           | Right    ]
    [ BCBUS3  | 54           | Right    ]
    [ BCBUS4  | 55           | Right    ]
    [ BCBUS5  | 57           | Right    ]
    [ BCBUS6  | 58           | Right    ]
    [ BCBUS7  | 59           | Right    ]

    [ PWREN   | 60           | Right    ]
    [ SUSPEND | 36           | Right    ]


  val box = BoxSymbol(self)
  assign-symbol $ create-symbol(box)

  val pkg = QFP(
    num-leads = 64,
    lead-profile = QFP-Lead-Profile(
      span = 12.0 +/- 0.25
      pitch = 0.5,
      lead-length = 0.6 +/- 0.15,
      lead-width = 0.22 +/- 0.05
    ),
    thermal-lead? = false,
    package-body = PackageBody(
      width = 10.0 +/- 0.1,
      length = 10.0 +/- 0.1,
      height = 1.50 +/- 0.1,
    )
  )

  assign-landpattern $ create-landpattern(pkg)

public pcb-module device () :

  public inst C : component()
